{
  "name": "video-player-browser",
  "version": "1.0.0",
  "description": "A Node.js app for browsing files and watching MP4 videos",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:start:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop video-player",
    "pm2:restart": "pm2 restart video-player",
    "pm2:reload": "pm2 reload video-player",
    "pm2:delete": "pm2 delete video-player",
    "pm2:logs": "pm2 logs video-player",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:resurrect": "pm2 resurrect"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "express-session": "^1.17.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "video",
    "player",
    "file",
    "browser",
    "mp4"
  ],
  "author": "",
  "license": "MIT"
}